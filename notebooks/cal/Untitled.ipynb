    "#\n",
    "# the average distance from an arbitrary point on a circle\n",
    "#\n",
    "%matplotlib inline\n",
    "import matplotlib.pylab as plt\n",
    "import numpy as np\n",
    "\n",
    "theta = np.linspace(0.0,2.0*np.pi,100)\n",
    "d_theta = np.sqrt(2.0 - 2*np.cos(theta))\n",
    "plt.plot(d_theta)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
